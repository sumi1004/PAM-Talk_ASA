{
  "version": 2,
  "builds": [
    {
      "src": "demo_dashboard.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/demo_dashboard.html"
    },
    {
      "src": "/config/keys_public.json",
      "dest": "/config/keys_public.json"
    },
    {
      "src": "/(.*\\.(md|html|css|js|json))",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
